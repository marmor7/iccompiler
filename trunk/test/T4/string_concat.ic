class StringConcat {

	string concatHello(string s) {
		string x ="Hello" + s; 
		Library.println(x);
		return (x );
	}

	static void main(string[] args) {
		StringConcat sc = new StringConcat();
		int x = 22;
		x =3;
		Library.printi(x);	
		string s = " world";
		Library.println(s);
		s = sc.concatHello(s);
		Library.println(s);
		s = sc.concatHello(s);
		Library.println(s);
		s = sc.concatHello(s);
		Library.println(s);
	}
}